  <method CLSNAME="ZCL_TRANSFER_ORDER" CMPNAME="INIT_ITEMS" VERSION="1" LANGU="E" DESCRIPT="Initialize Transfer Order items" EXPOSURE="1" STATE="1" EDITORDER="3 " DISPID="0 " AUTHOR="ESPERANCAB" CREATEDON="20140321" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="731" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD INIT_ITEMS.

  DATA:
        to_items  TYPE TABLE OF ltap,
        to_item   LIKE LINE OF to_items.

  &quot;Select entries from table LTAP
  SELECT * FROM ltap
    INTO TABLE to_items
    WHERE lgnum = me-&gt;data-lgnum
      AND tanum = me-&gt;data-tanum.

  &quot;Create item objects
  DATA:
        to_itm_key  TYPE bapitransorderkey,
        to_item_obj TYPE REF TO zcl_transfer_order_item.
  LOOP AT to_items INTO to_item.

    to_itm_key-whsenumber = to_item-lgnum.
    to_itm_key-trans_ord = to_item-tanum.
    to_itm_key-to_item = to_item-tapos.

    to_item_obj = zcl_transfer_order_item=&gt;get_instance(
                    to_itm_key ).
    APPEND to_item_obj TO me-&gt;items.

  ENDLOOP.
ENDMETHOD.</source>
   <methodDocumentation/>
  </method>
