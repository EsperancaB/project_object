   <source>METHOD GET_INSTANCE.

  SELECT SINGLE * FROM ltak
    INTO r_header_instance-&gt;data
    WHERE lgnum = i_header_key-whsenumber
      AND tanum = i_header_key-transferorderno.
  IF sy-subrc &lt;&gt; 0.
    RAISE EXCEPTION TYPE zcx_transfer_order_not_found
      EXPORTING
        header_key = i_header_key.
  ENDIF.

  CREATE OBJECT r_header_instance.
  r_header_instance-&gt;init_transfer_order_data( ).

ENDMETHOD.</source>
