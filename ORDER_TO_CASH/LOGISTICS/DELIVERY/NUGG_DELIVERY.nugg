   <parameter CLSNAME="ZCL_DELIVERY_ITEM" CMPNAME="GET_INSTANCE" SCONAME="I_ITEM_KEY" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="ESPERANCAB" CREATEDON="20140312" CHANGEDBY="ESPERANCAB" CHANGEDON="20140322" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BAPIPOSTAB"/>
   <parameter CLSNAME="ZCL_DELIVERY_ITEM" CMPNAME="GET_INSTANCE" SCONAME="R_ITEM_INSTANCE" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="ESPERANCAB" CREATEDON="20140312" CHANGEDBY="ESPERANCAB" CHANGEDON="20140322" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="3" TYPE="ZCL_DELIVERY_ITEM"/>
   <exception CLSNAME="ZCL_DELIVERY_ITEM" CMPNAME="GET_INSTANCE" SCONAME="ZCX_DELIVERY_ITEM_NOT_FOUND" VERSION="1" LANGU="E" DESCRIPT="Delivery Item not found" MTDTYPE="0" EDITORDER="1 " AUTHOR="ESPERANCAB" CREATEDON="20140312" CHANGEDBY="ESPERANCAB" CHANGEDON="20140322"/>
   <source>METHOD get_instance.

  CREATE OBJECT r_item_instance.

  SELECT SINGLE * FROM lips
    INTO r_item_instance-&gt;data
    WHERE vbeln = i_item_key-deliv_numb
      AND posnr = i_item_key-itm_number.
  IF sy-subrc &lt;&gt; 0.

    RAISE EXCEPTION TYPE zcx_delivery_item_not_found
      EXPORTING
        item_key = i_item_key.

  ENDIF.

  r_item_instance-&gt;init_doc_flow( ).

ENDMETHOD.</source>
