  </method>
  <method CLSNAME="ZCL_DELIVERY_ITEM" CMPNAME="INIT_DOC_FLOW" VERSION="1" LANGU="E" DESCRIPT="Initialize document flow" EXPOSURE="1" STATE="1" EDITORDER="2 " DISPID="0 " AUTHOR="ESPERANCAB" CREATEDON="20140312" CHANGEDBY="ESPERANCAB" CHANGEDON="20140313" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="731" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD init_doc_flow.

  &quot;Get subsequent docs
  SELECT * FROM vbfa
    INTO TABLE subsequent_docs
    WHERE vbelv = data-vbeln
      AND posnv = data-posnr.

  &quot;Get preceding docs
  SELECT * FROM vbfa
    INTO TABLE preceding_docs
    WHERE vbeln = data-vbeln
      AND posnn = data-posnr.

ENDMETHOD.</source>
