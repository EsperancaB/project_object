  <method CLSNAME="ZCL_DELIVERY_ITEM" CMPNAME="GET_TRANSFER_ORDER" VERSION="1" LANGU="E" DESCRIPT="Get transfer order" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " AUTHOR="ESPERANCAB" CREATEDON="20140312" CHANGEDBY="ESPERANCAB" CHANGEDON="20140322" MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_DELIVERY_ITEM" CMPNAME="GET_TRANSFER_ORDER" SCONAME="R_TRANSFER_ORDER" VERSION="1" LANGU="E" DESCRIPT="Transfer Order Header" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="ESPERANCAB" CREATEDON="20140312" CHANGEDBY="ESPERANCAB" CHANGEDON="20140322" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="3" TYPE="ZCL_TRANSFER_ORDER"/>
   <exception CLSNAME="ZCL_DELIVERY_ITEM" CMPNAME="GET_TRANSFER_ORDER" SCONAME="ZCX_TRANSFER_ORDER" VERSION="1" LANGU="E" DESCRIPT="Error with Transfer Order header" MTDTYPE="0" EDITORDER="1 " AUTHOR="ESPERANCAB" CREATEDON="20140312" CHANGEDBY="ESPERANCAB" CHANGEDON="20140322"/>
   <source>METHOD get_transfer_order.

  DATA:
        doc     LIKE LINE OF me-&gt;subsequent_docs,
        counter TYPE i.

  LOOP AT me-&gt;subsequent_docs INTO doc
    WHERE vbtyp_n = &apos;Q&apos; &quot;Transfer order type
      AND rfmng &lt;&gt; 0. &quot;Quantity not zero

    ADD 1 TO counter.

  ENDLOOP.

  &quot;There should only be one?
  IF counter &lt;&gt; 1.
    RAISE EXCEPTION TYPE zcx_transfer_order.
  ENDIF.

  DATA:
        to_key TYPE bapi2018a. &quot;transfer order key
  to_key-whsenumber = doc-lgnum.
  to_key-transferorderno = doc-vbeln.
  r_transfer_order = zcl_transfer_order=&gt;get_instance(
                       i_header_key = to_key ).

ENDMETHOD.</source>
